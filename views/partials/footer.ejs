        <script>
            //make current page active in nav bar
            var current = location.pathname;
            current = current.substring(1);
            $('nav * a').each(function(){
                var $this = $(this);
                // if the current path is like this link, make it active
                if($this.attr('href').indexOf(current) !== -1){
                    $this.addClass('active');
                }
            })
        </script>

